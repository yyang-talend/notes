{
  "name": "notes",
  "version": "1.0.0",
  "description": "Notes",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "loppo --site \"Notes\" --id notes --theme wangdoc",
    "build-and-commit": "npm run build && npm run commit",
    "commit": "gh-pages --dist dist --dest dist/notes --branch master --repo git@github.com:wangdoc/website.git",
    "chapter": "loppo chapter",
    "server": "loppo server"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm update"
    }
  },
  "keywords": [
    "java"
  ],
  "author": "Yang Yongsheng",
  "license": "CC-BY-SA-4.0",
  "homepage": "https://github.com/yyang-talend/notes#readme",
  "bugs": {
    "url": "https://github.com/yyang-talend/notes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyang-talend/notes.git"
  },
  "dependencies": {
    "gh-pages": "^3.2.3",
    "husky": "^4.3.8",
    "loppo": "^0.6.24",
    "loppo-theme-wangdoc": "^0.6.1"
  }
}
